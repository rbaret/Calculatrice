        <Button x:Name="button6" Content="6" HorizontalAlignment="Left" Margin="141,219,0,0" VerticalAlignment="Top" Width="60" Height="60" Click="CalcBtn_Click" FontSize="25" Background="#FFDFF9FF" BorderBrush="#FFFFCECE"/>
        <Button x:Name="button5" Content="5" HorizontalAlignment="Left" Margin="76,219,0,0" VerticalAlignment="Top" Width="60" Height="60" Click="CalcBtn_Click" FontSize="25" Background="#FFDFF9FF" BorderBrush="#FFFFCECE"/>
