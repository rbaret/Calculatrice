        <Button x:Name="button1" Content="1" HorizontalAlignment="Left" Margin="10,284,0,0" VerticalAlignment="Top" Width="60" Height="60" Click="CalcBtn_Click" FontSize="25" Background="#FFDFF9FF" BorderBrush="#FFFFCECE"/>
        <Button x:Name="button0" Content="0" HorizontalAlignment="Left" Margin="10,350,0,0" VerticalAlignment="Top" Width="60" Height="60" Click="CalcBtn_Click" FontSize="25" Background="#FFDFF9FF" BorderBrush="#FFFFCECE"/>
        <Button x:Name="buttonDecimal" Content="." HorizontalAlignment="Left" Margin="76,350,0,0" VerticalAlignment="Top" Width="60" Height="60" Click="CalcBtn_Click" FontSize="25" Background="#FFDFF9FF" BorderBrush="#FFFFCECE"/>
        <Button x:Name="buttonTotal" Content="=" HorizontalAlignment="Left" Margin="141,350,0,0" VerticalAlignment="Top" Width="60" Height="60" Click="CalcBtn_Click" FontSize="25" Background="#FFDFF9FF" BorderBrush="#FFFFCECE"/>
        <Button x:Name="buttonDivide" Content="/" HorizontalAlignment="Left" Margin="207,154,0,0" VerticalAlignment="Top" Width="60" RenderTransformOrigin="0.213,0.75" Height="60" Click="CalcBtn_Click" FontSize="25" Background="#FFDFF9FF" BorderBrush="#FFFFCECE"/>
        <Button x:Name="buttonMultiply" Content="*" HorizontalAlignment="Left" Margin="207,219,0,0" VerticalAlignment="Top" Width="60" Height="60" Click="CalcBtn_Click" FontSize="25" Background="#FFDFF9FF" BorderBrush="#FFFFCECE"/>
        <Button x:Name="buttonSubstract" Content="-" HorizontalAlignment="Left" Margin="207,284,0,0" VerticalAlignment="Top" Width="60" Height="60" Click="CalcBtn_Click" FontSize="25" Background="#FFDFF9FF" BorderBrush="#FFFFCECE"/>
