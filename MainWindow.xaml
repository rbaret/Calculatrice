        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Calculatrice"
        mc:Ignorable="d"
        Title="MainWindow" Height="453.614" Width="606" KeyDown="OnKeyDown">
    <Grid Margin="0,0,2,0">
        <Button x:Name="button9" Content="9" HorizontalAlignment="Left" Margin="141,154,0,0" VerticalAlignment="Top" Width="60" Height="60" Click="CalcBtn_Click" FontSize="25" Background="#FFDFF9FF" BorderBrush="#FFFFCECE"/>
        <Button x:Name="button8" Content="8" HorizontalAlignment="Left" Margin="76,154,0,0" VerticalAlignment="Top" Width="60" Height="60" Click="CalcBtn_Click" FontSize="25" Background="#FFDFF9FF" BorderBrush="#FFFFCECE"/>
